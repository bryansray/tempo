<table class="tps-list">
	<thead>
		<tr>
			<th>Date</th>
			<th>User</th>
			<th>Field</th>
			<th>Original</th>
			<th>New</th>
		</tr>
	</thead>
	<tbody>
	<% history.each do |item| %>
		<% i = 0
		   item.changes.each do |change|
			i = i + 1
			if i == 1 %>
			<tr>
				<td><%= item.created_at.strftime( "%m/%d/%y %H:%M" ) %></td>
				<td><%= item.user %></td>
			<% else %>
			<tr>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			<% end %>
			<% pretty = prettify_history_hash( change ) %>
				<td><%=h pretty[0] %></td>
				<td><%=h pretty[1][0] %></td>
				<td><%=h pretty[1][1] %></td>
			</tr>
		<% end %>
	<% end %>
	</tbody>
</table>